{
    "problemMatcher": [
        {
            "owner": "check-spelling-https",
            "pattern": [
                {
                    "regexp": "^(https://.+)#:(\\d+):(\\d+) \\.{3} (\\d+),\\s(Error|Warning|Notice)\\s-\\s(.+\\s\\((.+)\\))$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "endColumn": 4,
                    "severity": 5,
                    "message": 6,
                    "code": 7
                }
            ]
        },
        {
            "owner": "check-spelling-jq",
            "pattern": [
                {
                    "regexp": "^(jq): (error) \\(at ([^:]*):(\\d+)\\): (.*)$",
                    "file": 3,
                    "line": 4,
                    "severity": 2,
                    "message": 5,
                    "code": 1
                }
            ]
        },
        {
            "owner": "check-spelling-curl",
            "pattern": [
                {
                    "regexp": "^(curl): (\\d+) (.*)$",
                    "severity": 2,
                    "message": 3,
                    "code": 1
                }
            ]
        },
        {
            "owner": "check-spelling-git",
            "pattern": [
                {
                    "regexp": "^(fatal): ([^.'\"]+)(.*)$",
                    "severity": 1,
                    "message": 3,
                    "code": 2
                }
            ]
        },
        {
            "owner": "check-spelling-perl-not-enough-arguments",
            "pattern": [
                {
                    "regexp": "^(Not enough arguments for.* at (.*?) line (\\d+))",
                    "message": 1,
                    "file": 2,
                    "line": 3
                }
            ]
        },
        {
            "owner": "check-spelling-perl-compilation-failed",
            "pattern": [
                {
                    "regexp": "^(Compilation failed in.* at (.*?) line (\\d+))",
                    "message": 1,
                    "file": 2,
                    "line": 3
                }
            ]
        }
    ]
}

